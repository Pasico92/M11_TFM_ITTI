      "Code": "# BIBLIOTECAS\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\r\nimport clr  # CommonLanguage Runtime\r\n\r\n# Para trabajar con la RevitAPI\r\nclr.AddReference('RevitAPI')\r\nimport Autodesk\r\nfrom Autodesk.Revit.DB import *\r\n\r\n# Para trabajar contra el documento y hacer transacciones\r\nclr.AddReference('RevitServices')\r\nimport RevitServices\r\nfrom RevitServices.Persistence import DocumentManager\r\nfrom RevitServices.Transactions import TransactionManager\r\n# Identificadores\r\ndoc = DocumentManager.Instance.CurrentDBDocument\r\nuiapp = DocumentManager.Instance.CurrentUIApplication\r\napp = uiapp.Application\r\nuidoc = uiapp.ActiveUIDocument\r\n\r\n# Otras bibliotecas\r\nimport System\r\nfrom System.Collections.Generic import *  # Para generar iList\r\n\r\n# CODIGO\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\r\n# Accedemos al archivo txt que sale por defecto en nuestro documento\r\n# sharedParam = app.OpenSharedParameterFile()\r\n\r\n# Accedemos a la ruta del archivo txt que sale por defecto en nuestro documento\r\narchivoOrig = app.SharedParametersFilename\r\n\r\n# Sobreescribimos la ruta mediante el input IN[0]\r\narchivoOrig = IN[0]\r\n\r\n# Abrimos el archivo enlazado\r\nsharedParam = app.OpenSharedParameterFile()\r\n\r\n# Creamos el grupo de parametros del txt si no existe\r\nif sharedParam.Groups.get_Item(\"ClashDetection\"):\r\n    grupo3D = sharedParam.Groups.get_Item(\"ClashDetection\")\r\nelse:\r\n    grupo3D = sharedParam.Groups.Create(\"ClashDetection\")\r\n\r\n# Crear parametro compartido \"External Definition\" en el txt si no existe\r\n# Version Revit 2022 en adelante\r\nif grupo3D.Definitions.get_Item(\"3D\"):\r\n    definicion = grupo3D.Definitions.get_Item(\"3D\")\r\nelse:\r\n    extOpc = ExternalDefinitionCreationOptions(\"3D\", SpecTypeId.String.Text)\r\n    extOpc.UserModificable = True\r\n    extOpc.Description = \"Parametro añadido desde Python\"\r\n    definicion = grupo3D.Definitions.Create(extOpc)\r\n\r\n# Creamos conjunto de categorias fuertemente tipado\r\ncatSet = app.Create.NewCategorySet()\r\n\r\n# Obtenemos las categorias desde su BuiltInCategory\r\n# Arquitectura\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Floors))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Ramps))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Windows))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Doors))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Ceilings))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_CurtainWallPanels))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_CurtainWallMullions))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Railings))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Roofs))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Stairs))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Walls))\r\n\r\n# Estructura\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralFoundation))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralFraming))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralColumns))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Columns))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_CeilingsStructure))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralTendons))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralTruss))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_StructuralFramingSystem))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_TemporaryStructure))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_FloorsStructure))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_RoofsStructure))\r\n# catSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_WallsStructure))\r\n\r\n# MEP\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_PipeCurves))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_PipeFitting))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_PipeAccessory))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_FlexPipeCurves))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_DuctCurves))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_DuctFitting))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_DuctAccessory))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_FlexDuctCurves))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_DuctTerminal))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_MechanicalEquipment))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_PlumbingFixtures))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Sprinklers))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_CableTray))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_CableTrayFitting))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_Conduit))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_ConduitFitting))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_ElectricalEquipment))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_LightingDevices))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_LightingFixtures))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_DataDevices))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_FireAlarmDevices))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_ElectricalFixtures))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_SpecialityEquipment))\r\ncatSet.Insert(Category.GetCategory(doc, BuiltInCategory.OST_MedicalEquipment))\r\n\r\n# Creamos enlace \"Binding\"\r\n# TypeBinding: para parametros de Tipo\r\n# InstanceBinding: para parametros de Ejemplar\r\ninsBin = app.Create.NewInstanceBinding(catSet)\r\n\r\n# Definimos el \"Agrupar parametro en\", en este caso \"Otros\"\r\ngrupoProp = BuiltInParameterGroup.PG_TEXT\r\n\r\nTransactionManager.Instance.EnsureInTransaction(doc)\r\n# abrimos transaccion\r\n\r\n# Insertamos el parametro compartido como parametro de proyecto en el documento\r\ndoc.ParameterBindings.Insert(definicion, insBin, grupoProp)\r\n\r\n# Reinsertar un parametro compartido como parametro de proyecto porque se ha modificado\r\n# doc.ParameterBindings.ReInsert(definicion, insBin, grupoProp)\r\n\r\nTransactionManager.Instance.TransactionTaskDone()\r\n# cerramos transaccion\r\n\r\n# Insertamos el parametro compartido como parametro de proyecto en los vinculos\r\n# Colectamos los documentos de los vinculos\r\nvinculos = FilteredElementCollector(doc).OfClass(RevitLinkInstance)\r\ndocVinculos = [v.GetLinkDocument() for v in vinculos]\r\nPathdocVinculos = [v.GetLinkDocument().PathName for v in vinculos]\r\n\r\nTransactionManager.Instance.ForceCloseTransaction()\r\n# forzamos transaccion\r\n\r\nfor x in PathdocVinculos:\r\n    linkDoc = app.OpenDocumentFile(x)\r\n    \r\n    \r\n    \r\n    linkDoc.Close()\r\n    linkDoc.Reload()\r\n\r\n\r\n\r\n\r\n\r\n\r\n\"\"\"\r\nTransactionManager.Instance.EnsureInTransaction(doc)\r\n# abrimos transaccion\r\n\r\nfor d in docVinculos:\r\n    d.ParameterBindings.Insert(definicion, insBin, grupoProp)\r\n\r\nTransactionManager.Instance.TransactionTaskDone()\r\n# cerramos transaccion\r\n\r\n# SALIDA\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\"\"\"",
