      "Code": "# BIBLIOTECAS\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\r\nimport clr  # CommonLanguage Runtime\r\n\r\n# Para trabajar con la RevitAPI\r\nclr.AddReference('RevitAPI')\r\nimport Autodesk\r\nfrom Autodesk.Revit.DB import *\r\n\r\n# Para trabajar contra el documento y hacer transacciones\r\nclr.AddReference('RevitServices')\r\nimport RevitServices\r\nfrom RevitServices.Persistence import DocumentManager\r\nfrom RevitServices.Transactions import TransactionManager\r\n# Identificadores\r\ndoc = DocumentManager.Instance.CurrentDBDocument\r\nuiapp = DocumentManager.Instance.CurrentUIApplication\r\napp = uiapp.Application\r\nuidoc = uiapp.ActiveUIDocument\r\n\r\n# Otras bibliotecas\r\nimport System\r\nfrom System.Collections.Generic import *  # Para generar iList\r\n\r\n# CODIGO\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\r\n# Accedemos al archivo txt que sale por defecto en nuestro documento\r\n# sharedParam = app.OpenSharedParameterFile()\r\n# Metodo \"OpenSharedParameterFile()\": Enables access to shared parameter groups and definitions that are maintained on disk\r\n\t# sin Argumentos\r\n\t# cuelga de \"ControlledApplication Class\": Represents the Autodesk Revit Application with no access to documents. It provides options and other application wide data and settings for external applications OnStartup/OnShutdown\r\n\r\n# Accedemos a la ruta del archivo txt que sale por defecto en nuestro documento\r\narchivoOrg = app.SharedParameterFileName\r\n# Propiedad \"SharedParameterFileName\": Contains the fully qualified path to a shared parameters file\r\n\t# cuelga de \"ControlledApplication Class\": Represents the Autodesk Revit Application with no access to documents. It provides options and other application wide data and settings for external applications OnStartup/OnShutdown\r\n\r\n# Sobreescribimos la ruta mediante el input IN[0]\r\napp.SharedParameterFileName = IN[0]\r\n\r\n# Accedemos a los grupos de parametros del txt\r\nSharedParameterFile = app.OpenSharedParameterFile()\r\n# Metodo \"OpenSharedParameterFile()\": Enables access to shared parameter groups and definitions that are maintained on disk\r\n\t# sin Argumentos\r\n\t# cuelga de \"Application Class\": Represents the Autodesk Revit Application, providing access to documents, options and other application wide data and settings\r\n\r\n# Seleccionamos el grupo de parametros \"Dynamo\" dentro del txt\r\ngrupo = SharedParameterFile.Groups.get_Item(\"Dynamo\")\r\n\r\n# Creamos un nuevo grupo de parametros \"ITTI_2022\"\r\nSharedParameterFile.Groups.Create(\"ITTI_2022\")\r\n\r\n# Seleccionar el parametro compartido \"PruebaCargaDynamo\" dentro del grupo \"Dynamo\"\r\ndefinicion = grupo.Definitions.get_Item(\"PruebaCargaDynamo\")\r\n\r\n# Crear parametro compartido \"External Definition\" en el txt\r\n# Crear opciones y definicion 2021\r\nopc2021 = ExternalDefinitionCreationOptions(\"NombreParam\", ParameterType.Text)\r\nopc2021.UserModificable = True\r\nopc2021.Description = \"Parametro añadido desde Python\"\r\ndefinicion = grupo.Definition.Create(opc2021)\r\n\r\n# Version Revit 2022 en adelante\r\nopc2022 = ExternalDefinitionCreationOptions(\"NombreParam\", SpecTypeId.String.Text)\r\nopc2022.UserModificable = True\r\nopc2022.Description = \"Parametro creado desde Python\"\r\ndefinicion = grupo.Definitions.Create(opc2022)\r\n\r\n# Categorias a las que asignar el parametro\r\n# Creamos conjunto de categorias fuertemente tipado\r\ncatSet1 = app.Create.NewCategorySet()\r\n# Metodo \"NewCategorySet()\": Creates a new instance of a set specifically for holding category objects\r\n\t# sin Argumentos\r\n\t# cuelga de \"Application Class\": Represents the Autodesk Revit Application, providing access to documents, options and other application wide data and settings\r\n\r\n# Creamos conjunto de categorias fuertemente tipado (alternativa a \"NewCategorySet()\")\r\ncatSet2 = CategorySet()\r\n\r\n# Obtenemos la categoria de suelos desde su BuiltInCategory\r\ncat = Category.GetCategory(doc, BuiltInCategory.OST_Floors)\r\n\r\n# Insertamos la categoria en el conjunto\r\ncatSet1.Insert(cat)\r\ncatSet2.Insert(cat)\r\n\r\n# Crear enlace \"Binding\"\r\n# TypeBinding: para parametros de Tipo\r\ntypBin1 = app.Create.NewTypeBinding(catSet1)\r\ntypBin2 = TypeBinding(catSet2)\r\n\r\n# InstanceBinding: para parametros de Ejemplar\r\ninsBin1 = app.Create.NewInstanceBinding(catSet1)\r\ninsBin2 = InstanceBinding(catSet2)\r\n\r\n# Definimos el \"Agrupar parametro en\", en este caso \"Texto\"\r\ngrupoDeseado = BuiltInParameterGroup.PG_TEXT\r\n\r\n# Cuando no queremos asignar ningun grupo\r\n# BuiltInParameterGroup.INVALID\r\n\r\nTransactionManager.Instance.EnsureInTransaction(doc)\r\n# abrimos transaccion\r\n\r\n# Insertamos el parametro compartido como parametro de proyecto\r\ndoc.ParameterBindings.Insert(definicion, insBin2, grupoDeseado)\r\n\r\n# Reinsertar un parametro compartido como parametro de proyecto porque se ha modificado\r\ndoc.ParameterBindings.ReInsert(definicion, insBin2, grupoDeseado)\r\n\r\nTransactionManager.Instance.TransactionTaskDone()\r\n# cerramos transaccion\r\n\r\n# SALIDA\r\n# <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<\r\n\r\nOUT = insBin1, insBin2",
